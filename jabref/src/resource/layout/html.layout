<div class="bibtex_key"><a name="\bibtexkey">[\bibtexkey]</a></div>
<div class="bibtex_content">
    <span class="author">
        \begin{author}      \format[net.sf.jabref.export.layout.format.ConvertSpecialCharactersForHTML]{\author} \end{author}
    </span>.
    <span class="title">
        \begin{title} \format[net.sf.jabref.export.layout.format.RemoveBrackets]{\title} \end{title}
    </span>

    \begin{journal},  <span class="journal">\journal</span> \end{journal}

    \begin{year}, <span class="year">\year</span>\end{year}
    \begin{volume}, <span class="volume">\volume</span>\end{volume}
    \begin{pages}, <span class="pages">\pages</span> \end{pages}
.</div>
